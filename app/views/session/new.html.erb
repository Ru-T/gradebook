<p id="notice"><%= notice %></p>

<h1>Login</h1>
<p>Please login below.</p>

    <%= form_tag(session_create_path) do %>

      <p><%= label_tag :email %>
      <%= email_field_tag :email %></p>

      <p><%= label_tag :password %>
      <%= password_field_tag :password %></p>

      <%= submit_tag "Log in" %>
    <% end %>
